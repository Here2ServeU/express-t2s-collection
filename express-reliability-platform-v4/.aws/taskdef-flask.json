{
  "family": "flask-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "REPLACE_ME_EXECUTION_ROLE_ARN",
  "taskRoleArn": "REPLACE_ME_TASK_ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "flask-api",
      "image": "REPLACE_ME_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "PORT",
          "value": "5000"
        }
      ]
    }
  ]
}